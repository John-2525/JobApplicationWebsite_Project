@page "/indivskills/create/"
@inject HttpClient _client
@inject NavigationManager _navManager
<h3 class="card-title">Create New Jobposts</h3>

<hr />
<br />
<@FormComponent ButtonText="Create Job Positng" indivskill="indivskill" OnValidSubmit="@CreateIndivSkill" />

@code{
    IndividSkills indivskill = new IndividSkills();

    protected async Task CreateIndivSkill()
    {
        await _client.PostAsJsonAsync(Endpoint.IndivSkillsEndpoint, indivskill);
        _navManager.NavigateTo("/indivSkills/");

    }
}
